{
  "ServiceEndpoints": {
    "PaymentServiceEndpoint": "http://payment/paymentAuth",
    "ShippingServiceEndpoint": "http://shipping/shipping"
  },
  "AppSettings": {
    "AnalyticsEnabled": true
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://0.0.0.0:8080"
      }
    }
  },
  "Data": {
    "MongoConnection": {
      "ConnectionString": "mongodb://orders-db:27017/",
      "Database": "data"
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
